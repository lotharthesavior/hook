{
  "name": "voku/php-hooks",
  "description": "A fork of the WordPress filters hook system rolled in to a class to be ported into any PHP-based system",
  "keywords": [
    "hook-system",
    "filters-system",
    "hooks",
    "actions",
    "filters"
  ],
  "type": "library",
  "homepage": "http://github.com/voku/php-hooks/",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Ohad Raz",
      "homepage": "https://github.com/bainternet",
      "role": "Developer"
    },
    {
      "name": "Lars Moelleken",
      "homepage": "http://www.moelleken.org/",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/voku/php-hooks/issues"
  },
  "require": {
    "php": "^8.0|^8.1|^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "voku\\helper\\": "src/voku/helper/",
      "tests\\": "tests/"
    }
  }
}
